<table>
  <% @wishlist.each do |wishlist| %>
  <tr>
    <td>
      <%= image_tag wishlist.item.avatar.url, style: "width:50px; height:50px"%>
    </td>
    <td>
      <%= link_to  wishlist.item.title, item_path(wishlist.item.id)%>
    </td>
    <td>
      <%= link_to "삭제", wishlist_path(wishlist.id), method: :delete %>
    </td>
  </tr>
  <% end %>
</table>
